{
  "name": "blog-client",
  "version": "1.0.0",
  "description": "My extraordinary Nuxt.js project",
  "author": "chenlong",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --host 0.0.0.0",
    "build": "nuxt build && rm -rf !(.nuxt|static|package.json|nuxt.config.js) && cnpm install --production && npm start && npm run pm2",
    "start": "nuxt start",
    "pm2": "cross-env NODE_ENV=production pm2 start npm --name blog-client -- start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "element-ui": "^2.4.11",
    "koa": "^2.6.2",
    "mavon-editor": "^2.6.17",
    "node-sass": "^4.11.0",
    "nuxt": "^2.3.4",
    "postcss-loader": "^3.0.0",
    "qs": "^6.6.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "vue-lazyload": "^1.2.6"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0",
    "scss-loader": "^0.0.1"
  }
